{
  "name": "Rename Heroku Apps in Dashboard",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://*.heroku.com/*", "https://*.reddit.com/*"],
      "js": ["content.js"],
      "css": ["style.css"],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": ["images/*"]
}
